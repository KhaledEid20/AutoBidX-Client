{
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "tailwindCSS.classAttributes": [
    "class",
    "className",
    "theme"
  ],
  "tailwindCSS.experimental.classRegex": [
    [
      "twMerge\\(([^)]*)\\)",
      "[\"'`]([^\"'`]*).*?[\"'`]"
    ],
    [
      "createTheme(?:<\\w+>)?\\s*\\(([^)]*)\\)",
      "{?\\s?[\\w].*:\\s*?[\"'`]([^\"'`]*).*?,?\\s?}?"
    ]
  ],
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#c20f0f",
    "activityBar.background": "#c20f0f",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#053b05",
    "activityBarBadge.foreground": "#e7e7e7",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#c20f0f",
    "statusBar.background": "#930b0b",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#c20f0f",
    "statusBarItem.remoteBackground": "#930b0b",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#930b0b",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#930b0b99",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#930b0b"
}